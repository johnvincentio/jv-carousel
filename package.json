{
  "name": "jv-carousel",
  "version": "1.0.8",
  "description": "Gatsby React Carousel",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --mode development",
    "build": "webpack --mode production --progress",
    "production": "rm -rf dist && WEBPACK_ENV=build npm run build",
    "serve": "serve -l 8065 dist"
  },
  "repository": {
    "type": "git",
    "url": "git://git@github.com:johnvincentio/jv-carousel.git"
  },
  "keywords": [
    "carousel",
    "slider",
    "infinite carousel",
    "responsive carousel",
    "react carousel",
    "react slider",
    "lazy loading"
  ],
  "author": "jv",
  "license": "MIT",
  "main": "./lib/react-infinite-carousel.min.js",
  "bugs": {
    "url": "https://github.com/jv-carousel/issues"
  },
  "homepage": "https://github.com/johnvincentio/jv-carousel#readme",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "acorn": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.15.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "gm-base64": "^1.1.1",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "serve": "^11.2.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.8.2"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-responsive-mixin": "^0.4.0"
  },
  "npmName": "jv-carousel",
  "npmFileMap": [
    {
      "basePath": "/lib/",
      "files": [
        "*.js"
      ]
    }
  ]
}
